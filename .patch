+@use '@angular/material' as mat;
+
+@import url('https://fonts.googleapis.com/css2?family=Inter:wght@300;400;500;600;700&family=Poppins:wght@500;600&family=Roboto+Mono:wght@400;500&display=swap');
+
+@include mat.core();
+
+$finpocket-primary: mat.m2-define-palette(mat.$m2-purple-palette, 500, 300, 700);
+$finpocket-accent: mat.m2-define-palette(mat.$m2-orange-palette, 500, 300, 700);
+$finpocket-warn: mat.m2-define-palette(mat.$m2-red-palette, 500);
+
+$finpocket-theme: mat.m2-define-dark-theme((
+  color: (
+    primary: $finpocket-primary,
+    accent: $finpocket-accent,
+    warn: $finpocket-warn,
+  ),
+));
+
+@include mat.all-component-themes($finpocket-theme);
+
+:root {
+  color-scheme: dark;
+  --finpocket-primary: #9c27b0;
+  --finpocket-accent: #ff9800;
+  --finpocket-surface: #111015;
+  --finpocket-surface-alt: #1b1b22;
+  --finpocket-border: rgba(255, 255, 255, 0.08);
+  --finpocket-text: #f6f2ff;
+  --finpocket-subtle: rgba(246, 242, 255, 0.72);
+  --finpocket-muted: rgba(246, 242, 255, 0.48);
+  --finpocket-gradient: radial-gradient(circle at 20% 20%, rgba(156, 39, 176, 0.25), transparent 55%),
+    radial-gradient(circle at 80% 10%, rgba(255, 152, 0, 0.22), transparent 60%),
+    linear-gradient(160deg, rgba(43, 39, 53, 0.8), rgba(12, 10, 16, 0.92));
+}
+
+*, *::before, *::after {
+  box-sizing: border-box;
+}
+
+html, body {
+  margin: 0;
+  padding: 0;
+  min-height: 100%;
+  background: var(--finpocket-surface);
+  color: var(--finpocket-text);
+  font-family: 'Inter', 'Roboto', 'Segoe UI', system-ui, -apple-system, sans-serif;
+  letter-spacing: 0.01em;
+}
+
+body {
+  display: block;
+}
+
+app-root {
+  display: block;
+  min-height: 100vh;
+}
+
+a {
+  color: inherit;
+  text-decoration: none;
+}
+
+::selection {
+  background: rgba(156, 39, 176, 0.35);
+  color: var(--finpocket-text);
+}
+
+.feature-shell {
+  display: flex;
+  flex-direction: column;
+  gap: 1.75rem;
+  padding-bottom: 3rem;
+}
+
+.feature-shell__header {
+  display: flex;
+  flex-direction: column;
+  gap: 1rem;
+}
+
+.feature-shell__title {
+  font-family: 'Poppins', 'Inter', sans-serif;
+  font-weight: 600;
+  font-size: clamp(2.2rem, 3vw, 2.8rem);
+  letter-spacing: -0.02em;
+  color: var(--finpocket-text);
+}
+
+.feature-shell__subtitle {
+  max-width: 68ch;
+  color: var(--finpocket-subtle);
+  font-size: 1.05rem;
+  line-height: 1.6;
+}
+
+.feature-shell__actions {
+  display: flex;
+  flex-wrap: wrap;
+  gap: 0.75rem;
+}
+
+.feature-shell__grid {
+  display: grid;
+  gap: 1.25rem;
+  grid-template-columns: repeat(auto-fit, minmax(240px, 1fr));
+}
+
+.feature-card {
+  height: 100%;
+  border-radius: 18px;
+  border: 1px solid var(--finpocket-border);
+  background: color-mix(in srgb, var(--finpocket-surface-alt) 92%, rgba(156, 39, 176, 0.08));
+  box-shadow: 0 18px 44px rgba(8, 8, 10, 0.55);
+  display: flex;
+  flex-direction: column;
+}
+
+.feature-card__accent {
+  width: 48px;
+  height: 4px;
+  border-radius: 999px;
+  background: linear-gradient(120deg, var(--finpocket-primary), var(--finpocket-accent));
+  margin-bottom: 1rem;
+}
+
+.feature-card__title {
+  font-weight: 600;
+  letter-spacing: -0.01em;
+  font-size: 1.1rem;
+}
+
+.feature-card__text {
+  color: var(--finpocket-muted);
+  line-height: 1.5;
+}
+
+.tag-chip {
+  display: inline-flex;
+  align-items: center;
+  padding: 0.25rem 0.75rem;
+  border-radius: 999px;
+  background: rgba(255, 152, 0, 0.16);
+  color: #ffcb6b;
+  font-weight: 500;
+  font-size: 0.75rem;
+  letter-spacing: 0.08em;
+  text-transform: uppercase;
+}
+
+mat-card.feature-card {
+  padding: 1.5rem;
+}
+
+mat-card-header {
+  padding: 0;
+  margin-bottom: 0.75rem;
+}
+
+mat-card-title, mat-card-subtitle {
+  color: inherit;
+}
+
+mat-card-subtitle {
+  color: var(--finpocket-muted);
+}
+
+@media (max-width: 959px) {
+  .feature-shell__grid {
+    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
+  }
+}
+
+@media (max-width: 599px) {
+  .feature-shell__title {
+    font-size: 2rem;
+  }
+
+  mat-card.feature-card {
+    padding: 1.25rem;
+  }
+}
